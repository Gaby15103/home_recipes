  "step_groups": [
    {
      "position": 0,
      "steps": [
        {
          "image_url": null,
          "translations": [
            {
              "language_code": "fr",
              "instruction": "Enrober le poulet de fécule et blanchir immédiatement 1 minute à l‘eau bouillante à grande ébullition. Rafraîchir et réserver."
            },
            {
              "language_code": "en",
              "instruction": ""
            }
          ],
          "position": 0
        },
        {
          "image_url": null,
          "translations": [
            {
              "language_code": "fr",
              "instruction": "Blanchir les crevettes à eau à grande ébullition pendant 1 minute. Rafraîchir et réserver."
            },
            {
              "language_code": "en",
              "instruction": ""
            }
          ],
          "position": 1
        },
        {
          "image_url": null,
          "translations": [
            {
              "language_code": "fr",
              "instruction": "Chauffer le wok à haute température."
            },
            {
              "language_code": "en",
              "instruction": ""
            }
          ],
          "position": 2
        },
        {
          "image_url": null,
          "translations": [
            {
              "language_code": "fr",
              "instruction": "Verser l'huile végétale et ajouter l'ail, les oignons, les poivrons, le céleri, les piments forts, le poulet et les crevettes."
            },
            {
              "language_code": "en",
              "instruction": ""
            }
          ],
          "position": 3
        },
        {
          "image_url": null,
          "translations": [
            {
              "language_code": "fr",
              "instruction": "Ajouter la pâte de curry rouge, le concentré pour bouillon de poulet en poudre, le sucre, le sel et le lait de coco et faire sauter la cuisson 1 à 2 minutes."
            },
            {
              "language_code": "en",
              "instruction": ""
            }
          ],
          "position": 4
        },
        {
          "image_url": null,
          "translations": [
            {
              "language_code": "fr",
              "instruction": "Dresser la préparation sur une assiette ovale, garnir de cresson et décorer le bord de l‘assiette avec des quartiers de citron ou de lime."
            },
            {
              "language_code": "en",
              "instruction": ""
            }
          ],
          "position": 5
        },
        {
          "image_url": null,
          "translations": [
            {
              "language_code": "fr",
              "instruction": "Parsemer les arachides, les feuilles de menthe et les oignons verts sur le dessus de la préparation."
            },
            {
              "language_code": "en",
              "instruction": ""
            }
          ],
          "position": 6
        },
        {
          "image_url": null,
          "translations": [
            {
              "language_code": "fr",
              "instruction": "Servir avec du Riz parfumé à la vapeur (voir page 105)."
            },
            {
              "language_code": "en",
              "instruction": ""
            }
          ],
          "position": 7
        }
      ],
